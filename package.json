{
  "name": "vue-check-password",
  "version": "1.0.4",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint vue-project --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.17",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vue-tsc": "^1.6.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Enquiet/password-complexity-check-vue3"
  },
  "bugs": {
    "url": "https://github.com/Enquiet/password-complexity-check-vue3/issues"
  },
  "homepage": "https://github.com/Enquiet/password-complexity-check-vue3#readme",
  "main": "dist/vue-check-password.mjs",
  "module": "dist/vue-check-password.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-check-password.mjs",
      "require": "./dist/vue-check-password.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vue",
    "vuejs",
    "component",
    "password",
    "check"
  ]
}
